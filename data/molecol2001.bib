@article{ ISI:000168367400008,
Author = {Pialek, J and Hauffe, HC and Rodriguez-Clark, KM and Searle, JB},
Title = {{Raciation and speciation in house mice from the Alps: the role of
   chromosomes}},
Journal = {{MOLECULAR ECOLOGY}},
