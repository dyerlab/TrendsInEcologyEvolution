@article{ ISI:000168367400017,
Author = {Nersting, LG and Arctander, P},
Title = {{Phylogeography and conservation of impala and greater kudu}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2001}},
Volume = {{10}},
Number = {{3}},
