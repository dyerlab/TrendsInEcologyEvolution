@article{ ISI:000167779400005,
Author = {Beaumont, M and Barratt, EM and Gottelli, D and Kitchener, AC and
   Daniels, MJ and Pritchard, JK and Bruford, MW},
Title = {{Genetic diversity and introgression in the Scottish wildcat}},
