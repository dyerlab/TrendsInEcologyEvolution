@article{ ISI:000171127700012,
Author = {Jensen-Seaman, MI and Kidd, KK},
Title = {{Mitochondrial DNA variation and biogeography of eastern gorillas}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2001}},
Volume = {{10}},
Number = {{9}},
Pages = {{2241-2247}},
Month = {{SEP}},
Abstract = {{Mitochondrial DNA variation in 109 individuals from four populations of
   wild living gorillas in East Africa was ascertained by sequencing the
   first hypervariable segment of the control region, or `D-loop'.
   amplified from noninvasively collected hair and faeces. D-loop
   haplotypes from eastern gorillas fell into two distinct clades, each
   with low levels of genetic diversity; most observed haplotypes within
   each clade differing by only one or two mutations. Both clades show
   evidence of population bottlenecks in the recent past, perhaps
   concomitant with the tropical forest reduction and fragmentation brought
   on by global cooling and drying associated with the last glacial
   maximum.}},
