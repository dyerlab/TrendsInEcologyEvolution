@article{ ISI:000178134500027,
Author = {Pinto, J and Donnelly, MJ and Sousa, CA and Gil, V and Ferreira, C and
   Elissa, N and Do Rosario, VE and Charlwood, JD},
Title = {{Genetic structure of Anopheles gambiae (Diptera : Culicidae) in Sao Tome
   and Principe (West Africa): implications for malaria control}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2002}},
Volume = {{11}},
Number = {{10}},
Pages = {{2183-2187}},
Month = {{OCT}},
Abstract = {{The impact of a vector eradication programme, conducted in the 1980s, on
   Anopheles gambiae populations from the islands of Sao Tome and Principe,
   was evaluated by microsatellite DNA analysis. Significant genetic
   differentiation was observed within and between the two islands and
   between the islands and a population from Gabon, suggesting a degree of
   isolation between them. Large estimates of long-term N-e suggested that
   the control programme did not affect the effective population size of
   the vector. Heterozygosity tests were also not consistent with a recent
   bottleneck.}},
