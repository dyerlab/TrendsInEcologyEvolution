@article{ ISI:000178962500022,
Author = {Leberg, PL},
Title = {{Estimating allelic richness: Effects of sample size and bottlenecks}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2002}},
Volume = {{11}},
Number = {{11}},
Pages = {{2445-2449}},
Month = {{NOV}},
Abstract = {{Although differences in sampling intensity can bias comparisons of
   allelic richness (A) among populations, investigators often fail to
   correct estimates of A for differences in sample size. Methods that
   standardize A on the basis of the size of the smallest number of samples
   in a comparison are preferable to other approaches. Rarefaction and
   repeated random subsampling provide unbiased estimates of A with the
   greatest precision and thus provide greatest statistical power to detect
   differences in variation. Less promising approaches, in terms of bias or
   precision, include single random subsampling, eliminating very small
   samples, using sample size as a covariate or extrapolating estimates
   obtained from small samples to a larger number of individuals.}},
