@article{ ISI:000177086900031,
Author = {Rico, C and Turner, GF},
Title = {{Extreme microallopatric divergence in a cichlid species from Lake Malawi}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2002}},
Volume = {{11}},
Number = {{8}},
Pages = {{1585-1590}},
Month = {{AUG}},
Abstract = {{We demonstrate significant population structuring on an extremely small
   spatial scale between adjacent demes of a Lake Malawi haplochromine
   cichlid species of the mbuna group, Pseudotropheus callainos, separated
   by only 35 m of habitat discontinuity. This substantiates further the
   notion that intralacustrine allopatric divergence may help to explain
   the high level of species richness of the mbuna in comparison to other
   Malawian cichlids, as well as of the Malawian haplochromines as a whole.}},
