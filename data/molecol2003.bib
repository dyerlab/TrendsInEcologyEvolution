@article{ ISI:000183514900025,
Author = {Vines, TH and Barton, NH},
Title = {{A new approach to detecting mixed families}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2003}},
Volume = {{12}},
Number = {{7}},
Pages = {{1999-2002}},
Month = {{JUL}},
Abstract = {{There are several analyses in evolutionary ecology which assume that a
   family of offspring has come from only two parents. Here, we present a
   simple test for detecting when a batch involves two or more subfamilies.
   It is based on the fact that the mixing of families generates
   associations amongst unlinked marker loci. We also present simulations
   illustrating the power of our method for varying numbers of loci,
   alleles per locus and genotyped individuals.}},
