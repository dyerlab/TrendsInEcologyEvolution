@article{ ISI:000183514900028,
Author = {Telfer, PT and Souquiere, S and Clifford, SL and Abernethy, KA and
   Bruford, MW and Disotell, TR and Sterner, KN and Roques, P and Marx, PA
   and Wickings, EJ},
Title = {{Molecular evidence for deep phylogenetic divergence in Mandrillus sphinx}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2003}},
Volume = {{12}},
Number = {{7}},
Pages = {{2019-2024}},
Month = {{JUL}},
Abstract = {{Mandrills (Mandrillus sphinx ) are forest primates indigenous to western
   central Africa. Phylogenetic analysis of 267 base pairs (bp) of the
   cytochrome b gene from 53 mandrills of known and 17 of unknown
   provenance revealed two phylogeographical groups, with haplotypes
   differentiated by 2.6\% comprising seven synonymous transitions. The
   distribution of the haplotypes suggests that the Ogooue River, Gabon,
   which bisects their range, separates mandrill populations in Cameroon
   and northern Gabon from those in southern Gabon. The haplotype
   distribution is also concordant with that of two known mandrill simian
   immunodeficiency viruses, suggesting that these two mandrill phylogroups
   have followed different evolutionary trajectories since separation.}},
