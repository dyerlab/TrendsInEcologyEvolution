@article{ ISI:000188825700005,
Author = {Zeller, KA and Bowden, RL and Leslie, JF},
Title = {{Population differentiation and recombination in wheat scab populations
   of Gibberella zeae from the United States}},
Journal = {{MOLECULAR ECOLOGY}},
