@article{ ISI:000223863000033,
Author = {Pamilo, P},
Title = {{How cryptic is the Scandinavian lynx?}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2004}},
Volume = {{13}},
Number = {{10}},
Pages = {{3257-3259}},
Month = {{OCT}},
Abstract = {{A reanalysis of the existing genotypic data shows that they are
   compatible with the hypothesis that the present-day Scandinavian lynx
   represents a recent expansion from a single source. This contradicts
   earlier conclusions. The difference arises when it is assumed that the
   expansion and colonization have been stochastic processes and that the
   populations may not be at a drift-migration equilibrium.}},
