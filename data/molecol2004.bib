@article{ ISI:000221302600010,
Author = {Moon, CD and Craven, KD and Leuchtmann, A and Clement, SL and Schardl,
   CL},
Title = {{Prevalence of interspecific hybrids amongst asexual fungal endophytes of
   grasses}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2004}},
