@article{ ISI:000188825700018,
Author = {Laloi, D and Richard, M and Lecomte, J and Massot, M and Clobert, J},
Title = {{Multiple paternity in clutches of common lizard Lacerta vivipara: data
   from microsatellite markers}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2004}},
Volume = {{13}},
Number = {{3}},
Pages = {{719-723}},
Month = {{MAR}},
Abstract = {{The common lizard (Lacerta vivipara) is a small live-bearing lacertid
   that reproduces once a year. In order to document the poorly known
   mating system of this species, we present here an assessment of multiple
   paternity using microsatellite markers. Paternities were established
   within 122 clutches belonging to two wild populations from contrasted
   areas and to four seminatural enclosed populations. The proportion of
   multiply sired clutches was found to be very high (between 50.0\% and
   68.2\%) and similar among populations, which suggests that the mating
   system of this species may be insensitive to environmental and
   population conditions.}},
