@article{ ISI:000224524000024,
Author = {Lidgard, DC and Boness, DJ and Bowen, WD and McMillan, JI and Fleischer,
   RC},
Title = {{The rate of fertilization in male mating tactics of the polygynous grey
   seal}},
Journal = {{MOLECULAR ECOLOGY}},
