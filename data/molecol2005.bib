@article{ ISI:000229961500025,
Author = {Asuka, Y and Tomaru, N and Munehara, Y and Tani, N and Tsumura, Y and
   Yamamoto, S},
Title = {{Half-sib family structure of Fagus crenata saplings in an old-growth
   beech-dwarf bamboo forest}},
Journal = {{MOLECULAR ECOLOGY}},
