@article{ ISI:000227721900017,
Author = {McDaniel, SF and Shaw, AJ},
Title = {{Selective sweeps and intercontinental migration in the cosmopolitan moss
   Ceratodon purpureus (Hedw.) Brid.}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2005}},
Volume = {{14}},
Number = {{4}},
Pages = {{1121-1132}},
