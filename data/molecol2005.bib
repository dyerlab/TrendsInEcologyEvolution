@article{ ISI:000231223000026,
Author = {Acevedo-Whitehouse, K and Vicente, J and Gortazar, C and Hofle, U and
   Fernandez-De-Mera, IG and Amos, W},
Title = {{Genetic resistance to bovine tuberculosis in the Iberian wild boar}},
