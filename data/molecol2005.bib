@article{ ISI:000227132300021,
Author = {Petit, RJ and Deguilloux, MF and Chat, J and Grivet, D and Garnier-Gere,
   P and Vendramin, GG},
Title = {{Standardizing for microsatellite length in comparisons of genetic
   diversity}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2005}},
Volume = {{14}},
Number = {{3}},
Pages = {{885-890}},
Month = {{MAR}},
Abstract = {{Mutation rates at microsatellites tend to increase with the number of
   repeats of the motif, leading to higher levels of polymorphism at long
   microsatellites. To standardize levels of diversity when microsatellites
   differ in size, we investigate the relationship between tract length and
   variation and provide a formula to adjust allelic richness to a fixed
   mean number of repeats in the specific case of chloroplast
   microsatellites. A comparison between 39 loci from eight species of
   conifers (where chloroplast DNA is paternally inherited) and 64 loci
   from 12 species of angiosperms (where chloroplast DNA is generally
   predominantly maternally inherited) indicates that the greater allelic
   richness found in conifers remains significant after controlling for
   number of repeats. The approach stresses the advantage of reporting
   variation in number of repeats instead of relative fragment sizes.}},
