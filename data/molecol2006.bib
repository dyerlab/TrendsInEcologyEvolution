@article{ ISI:000236205600024,
Author = {Avis, PG and Dickie, IA and Mueller, GM},
Title = {{A `dirty' business: testing the limitations of terminal restriction
   fragment length polymorphism (TRFLP) analysis of soil fungi}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2006}},
Volume = {{15}},
Number = {{3}},
Pages = {{873-882}},
Month = {{MAR}},
Abstract = {{Terminal restriction fragment length polymorphism (TRFLP) is an
   increasingly popular method in molecular ecology. However, several key
   limitations of this method have not been fully examined especially when
   used to study fungi. We investigated the impact of spore contamination,
   intracollection ribosomal DNA internal transcribed spacer (ITS) region
   variation, and conserved restriction enzyme recognition loci on the
   results produced by TRFLP to characterize soil fungal communities. We
   find that 6) the potential for nontarget structures such as spores to
   contribute DNA to target sample extractions is high; GO multiple
   fragments (i.e. `extra peaks') per PCR primer-restriction enzyme
   combination can be detected that are caused by restriction enzyme
   inefficiency and intracollection ribosomal DNA ITS variation; and (iii)
   restriction enzyme digestion in conserved vs. variable gene regions
   leads to different characterizations of community diversity. Based on
   these results, we suggest that studies employing TRFLP need to include
   information from known, identified fungi from sites within which studies
   take place and not to rely only on TRFLP profiles as a short cut to
   fungal community description.}},
