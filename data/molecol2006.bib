@article{ ISI:000234029800016,
Author = {Kozak, KH and Blaine, RA and Larson, A},
Title = {{Gene lineages and eastern North American palaeodrainage basins:
   phylogeography and speciation in salamanders of the Eurycea bislineata
   species complex}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2006}},
Volume = {{15}},
