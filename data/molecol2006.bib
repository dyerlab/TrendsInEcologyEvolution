@article{ ISI:000242042800019,
Author = {Rajakaruna, R. S. and Brown, J. A. and Kaukinen, K. H. and Miller, K. M.},
Title = {{Major histocompatibility complex and kin discrimination in Atlantic
   salmon and brook trout}},
Journal = {{MOLECULAR ECOLOGY}},
