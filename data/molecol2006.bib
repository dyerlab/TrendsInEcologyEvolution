@article{ ISI:000238143900014,
Author = {Roberts, TE},
Title = {{History, ocean channels, and distance determine phylogeographic patterns
   in three widespread Philippine fruit bats (Pteropodidae)}},
Journal = {{MOLECULAR ECOLOGY}},
