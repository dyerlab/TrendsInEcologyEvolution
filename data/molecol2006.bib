@article{ ISI:000236205600002,
Author = {Kumar, S and Gupta, J and Kumar, N and Dikshit, K and Navani, N and
   Jain, P and Nagarajan, M},
Title = {{Genetic variation and relationships among eight Indian riverine buffalo
   breeds}},
