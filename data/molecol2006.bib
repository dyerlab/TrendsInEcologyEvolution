@article{ ISI:000240440800014,
Author = {Albach, Dirk C. and Schoenswetter, Peter and Tribsch, Andreas},
Title = {{Comparative phylogeography of the Veronica alpina complex in Europe and
   North America}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2006}},
Volume = {{15}},
