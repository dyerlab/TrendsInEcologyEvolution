@article{ ISI:000247757200009,
Author = {Shaw, Kerry L. and Parsons, Yvonne M. and Lesnick, Sky C.},
Title = {{QTL analysis of a rapidly evolving speciation phenotype in the Hawaiian
   cricket Laupala}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2007}},
Volume = {{16}},
