@article{ ISI:000243305200006,
Author = {Gunnarsson, U. and Shaw, A. J. and Lonn, M.},
Title = {{Local-scale genetic structure in the peatmoss Sphagnum fuscum}},
Journal = {{MOLECULAR ECOLOGY}},
