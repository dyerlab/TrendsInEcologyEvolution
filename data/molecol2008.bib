Title = {{Stability of genetic variance and covariance for reproductive characters
   in the face of climate change in a wild bird population}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2008}},
Volume = {{17}},
Number = {{1}},
Pages = {{179-188}},
