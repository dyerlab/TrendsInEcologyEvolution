@article{ ISI:000259953500001,
Author = {Whiteman, Noah K.},
Title = {{Between a whale bone and the deep blue sea: the provenance of dwarf
   males in whale bone-eating tubeworms}},
Journal = {{MOLECULAR ECOLOGY}},
