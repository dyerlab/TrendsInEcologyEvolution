@article{ ISI:000255139700005,
Author = {Crispo, Erika and Chapman, Lauren J.},
Title = {{Population genetic structure across dissolved oxygen regimes in an
   African cichlid fish}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2008}},
