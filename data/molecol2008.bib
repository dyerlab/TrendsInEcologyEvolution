@article{ ISI:000256757800003,
Author = {Mancia, A. and Warr, W. and Chapman, R. W.},
Title = {{A transcriptomic analysis of the stress induced by capture-release
   health assessment studies in wild dolphins (Tursiops truncatus)}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2008}},
Volume = {{17}},
Number = {{11}},
Pages = {{2581-2589}},
Month = {{JUN}},
Abstract = {{The health of wild bottlenose dolphins (Tursiops truncatus) is typically
   evaluated by the study of animals that are captured and released back
   into the wild after examination. The impact of such studies on gene
   expression in peripheral blood cells was investigated using microarray
   and quantitative polymerase chain reaction methods. Significantly
   increased expression was observed in two major classes of genes: (i)
   energy metabolism, and (ii) responsiveness to stress and trauma, the
   latter effect suggesting the initiation of an acute-phase response. The
   value of data obtained in capture/release studies may need to be weighed
   against the potential physiological impacts of such studies.}},
