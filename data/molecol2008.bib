@article{ ISI:000256757800019,
Author = {Kennerly, Erin and Ballmann, Anne and Martin, Stanton and Wolfinger,
   Russ and Gregory, Simon and Stoskopf, Michael and Gibson, Greg},
Title = {{A gene expression signature of confinement in peripheral blood of red
   wolves (Canis rufus)}},
