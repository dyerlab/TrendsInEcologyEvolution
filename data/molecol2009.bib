@article{ ISI:000271468800006,
Author = {Galtier, N. and Nabholz, B. and Glemin, S. and Hurst, G. D. D.},
Title = {{Mitochondrial DNA as a marker of molecular diversity: a reappraisal}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2009}},
Volume = {{18}},
Number = {{22}},
Pages = {{4541-4550}},
Month = {{NOV}},
Abstract = {{Over the last three decades, mitochondrial DNA has been the most popular
   marker of molecular diversity, for a combination of technical
   ease-of-use considerations, and supposed biological and evolutionary
   properties of clonality, near-neutrality and clock-like nature of its
   substitution rate. Reviewing recent literature on the subject, we argue
   that mitochondrial DNA is not always clonal, far from neutrally evolving
   and certainly not clock-like, questioning its relevance as a witness of
   recent species and population history. We critically evaluate the usage
   of mitochondrial DNA for species delineation and identification.
   Finally, we note the great potential of accumulating mtDNA data for
   evolutionary and functional analysis of the mitochondrial genome.}},
