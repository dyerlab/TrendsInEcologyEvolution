@article{ ISI:000268760100003,
Author = {Bretman, Amanda and Newcombe, Devi and Tregenza, Tom},
Title = {{Promiscuous females avoid inbreeding by controlling sperm storage}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2009}},
Volume = {{18}},
Number = {{16}},
Pages = {{3340-3345}},
Month = {{AUG}},
Abstract = {{Recent studies in a variety of species have shown that polyandrous
   females are somehow able to bias paternity against their relatives
   postcopulation, although how they do so remains unknown. Field crickets
   readily mate with their siblings, but when also mated to an unrelated
   male, they produce disproportionately fewer inbred offspring. We use a
   new competitive microsatellite polymerase chain reaction technique to
   determine the contribution of males to stored sperm and subsequent
   paternity of offspring. Paternity is almost completely predicted by how
   much sperm from a particular male is stored, and unrelated males
   contribute more sperm to storage and have a corresponding higher
   paternity success.}},
