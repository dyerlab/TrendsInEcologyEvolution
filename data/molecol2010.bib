Language = {{English}},
Affiliation = {{Harr, B (Reprint Author), Max Planck Inst Evolutionary Biol, August Thienemannstr 2, D-24306 Plon, Germany.
   Harr, Bettina; Turner, Leslie M., Max Planck Inst Evolutionary Biol, D-24306 Plon, Germany.}},
DOI = {{10.1111/j.1365-294X.2009.04490.x}},
ISSN = {{0962-1083}},
