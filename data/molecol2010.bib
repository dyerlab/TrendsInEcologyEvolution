@article{ ISI:000285012300021,
Author = {Branco, Sara},
Title = {{Serpentine soils promote ectomycorrhizal fungal diversity}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2010}},
Volume = {{19}},
Number = {{24}},
Pages = {{5566-5576}},
Month = {{DEC}},
Abstract = {{Serpentine soils impose physiological stresses that limit plant
   establishment and diversity. The degree to which serpentine soils entail
   constraints on other organisms is, however, poorly understood. Here, I
   investigate the effect of serpentine soils on ectomycorrhizal (ECM)
   fungi by conducting a reciprocal transplant experiment, where serpentine
   and nonserpentine ECM fungal communities were cultured in both their
   native and non-native soils. Contrary to expectation, serpentine soils
   hosted higher fungal richness compared to nonserpentine, and most
   species were recovered from serpentine soil, suggesting ECM fungi are
   not overall specialized or strongly affected by serpentine edaphic
   constraints.}},
