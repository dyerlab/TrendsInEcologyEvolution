@article{ ISI:000295230000014,
Author = {Torres-Perez, Fernando and Eduardo Palma, R. and Hjelle, Brian and
   Holmes, Edward C. and Cook, Joseph A.},
Title = {{Spatial but not temporal co-divergence of a virus and its mammalian host}},
Journal = {{MOLECULAR ECOLOGY}},
