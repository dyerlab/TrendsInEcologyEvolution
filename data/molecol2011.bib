@article{ ISI:000297008300014,
Author = {Chaves, Jaime A. and Weir, Jason T. and Smith, Thomas B.},
Title = {{Diversification in Adelomyia hummingbirds follows Andean uplift}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2011}},
Volume = {{20}},
Number = {{21}},
Pages = {{4564-4576}},
Month = {{NOV}},
Abstract = {{The Andes are known to have influenced speciation patterns in many taxa,
   yet whether species diversification occurred simultaneously with their
   uplift or only after uplift was complete remains unknown. We examined
   both the phylogenetic pattern and dates of branching in Adelomyia
   hummingbirds in relation to Andean uplift to determine whether
   diversification coincides with the chronological phases of the uplift or
   with recent climatic fluctuations after Andean formation. Results
   suggest that the genus Adelomyia originated in the central Andes in the
   Miocene and was found to be comprised of six deeply divergent
   phylogroups dating between 3.5 and 6 Ma. The most basal splits in the
   tree, corresponding to the most southerly distributed of the six
   phylogroups, diverged in the late Miocene, whereas the northern
   phylogroups originated during the early-to-mid-Pliocene, when the
   northern Andes reached heights sufficient to support Adelomyia
   populations. Although we provide evidence for a southern origin for the
   group, the subsequent diversification of the northern phylogroups did
   not strictly follow the hypothesized south-to-north orogeny of the
   Andes. Further genetic structure within phylogroups may have resulted
   from Pleistocene climate fluctuations after the onset of the six
   lineages during the Mio-Pliocene. We explore the processes that promoted
   diversification in the Andes and suggest that in at least some groups,
   divergence was coupled to Andean orogeny.}},
