Address = {{COMMERCE PLACE, 350 MAIN ST, MALDEN 02148, MA USA}},
Type = {{Editorial Material}},
Language = {{English}},
Affiliation = {{Valkiunas, G (Reprint Author), Nat Res Ctr, Akademijos 2, LT-08412 Vilnius 21, Lithuania.
   Nat Res Ctr, LT-08412 Vilnius 21, Lithuania.}},
DOI = {{10.1111/j.1365-294X.2011.05187.x}},
ISSN = {{0962-1083}},
Keywords = {{biomedicine; birds; disease biology; ecological genetics; insects; life
   history evolution}},
Keywords-Plus = {{MORPHOLOGY; PARASITES}},
