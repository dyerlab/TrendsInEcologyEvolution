ISSN = {{0962-1083}},
Keywords = {{adaptation; behaviour; social evolution; evolutionary conflict; genetic
   architecture}},
Keywords-Plus = {{GAMETE-RECOGNITION; POSITIVE SELECTION; NATURAL-SELECTION; ADAPTIVE
