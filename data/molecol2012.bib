@article{ ISI:000306087100019,
Author = {Weckworth, Byron V. and Musiani, Marco and McDevitt, Allan D. and
   Hebblewhite, Mark and Mariani, Stefano},
Title = {{Reconstruction of caribou evolutionary history in Western North America
   and its implications for conservation}},
Journal = {{MOLECULAR ECOLOGY}},
