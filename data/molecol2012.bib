@article{ ISI:000308634300001,
Author = {Oldroyd, Benjamin P.},
Title = {{Domestication of honey bees was associated with expansion of genetic
   diversity}},
Journal = {{MOLECULAR ECOLOGY}},
Year = {{2012}},
Volume = {{21}},
Number = {{18}},
Pages = {{4409-4411}},
Month = {{SEP}},
Abstract = {{Humans have been keeping honey bees, Apis mellifera, in artificial hives
   for over 7000 years. Long enough, one might imagine, for some genetic
   changes to have occurred in domestic bees that would distinguish them
   from their wild ancestors. Indeed, some have argued that the recent
   mysterious and widespread losses of commercial bee colonies, are due in
   part to inbreeding. In this issue of Molecular Ecology, Harpur et al.
   (2012) show that the domestication of honey bees, rather than reducing
   genetic variance in the population, has increased it. It seems that the
   commercial honey bees of Canada are a mongrel lot, with far more
   variability than their ancestors in Europe.}},
